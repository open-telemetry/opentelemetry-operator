  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@opentelemetry/api": "1.3.0",
    "@opentelemetry/auto-instrumentations-node": "0.35.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "0.34.0",
    "@opentelemetry/sdk-node": "0.34.0"
  }
}
